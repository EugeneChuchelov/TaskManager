<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="window" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddWindowController">
    <children>
        <Label layoutX="41.0" layoutY="28.0" text="Название"/>
        <Label layoutX="40.0" layoutY="92.0" text="Описание"/>
        <Label layoutX="43.0" layoutY="148.0" text="Дата"/>
        <Label layoutX="42.0" layoutY="192.0" text="Время"/>
        <Label layoutX="43.0" layoutY="242.0" text="Контакты"/>
        <TextField fx:id="titleField" layoutX="290.0" layoutY="24.0"/>
        <TextField fx:id="descriptionField" layoutX="290.0" layoutY="88.0"/>
        <DatePicker fx:id="datePicker" layoutX="290.0" layoutY="144.0"/>
        <Spinner fx:id="hours" editable="true" layoutX="290.0" layoutY="190.0" prefWidth="70.0"/>
        <Spinner fx:id="minutes" editable="true" layoutX="380.0" layoutY="190.0" prefWidth="70.0"/>
        <TextField fx:id="contactsField" layoutX="290.0" layoutY="238.0"/>
        <Button fx:id="acceptButton" layoutX="144.0" layoutY="316.0" mnemonicParsing="false" onAction="#accept"
                text="Добавить" textAlignment="CENTER"/>
        <Button layoutX="365.0" layoutY="316.0" mnemonicParsing="false" onAction="#decline" text="Отмена"/>
        <Label layoutX="370.0" layoutY="195" text=":"/>
    </children>
</AnchorPane>
