<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="240.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HalfHourWindowController">
    <children>
        <GridPane gridLinesVisible="true" layoutX="7.0" layoutY="8.0" prefHeight="192.0" prefWidth="590.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label fx:id="titleLabel" text="Title" textAlignment="CENTER"/>
                <Label fx:id="descriptionLabel" text="Description" GridPane.rowIndex="1"/>
                <Label fx:id="timeLabel" text="Time" GridPane.rowIndex="2"/>
                <Label fx:id="contactsLabel" text="Contacts" GridPane.rowIndex="3"/>
            </children>
        </GridPane>
        <Button layoutX="286.0" layoutY="209.0" mnemonicParsing="false" onAction="#close" text="OK"/>
    </children>
</AnchorPane>
